
load "io".
load "vector".
load "util".

let triangle = [[1]].
let ix = 0.

for i in 1 to 6 do
    let v = triangle @ix.
    let v1 = [0] + v.
    let v2 = v + [0].
    let new_v = vadd(v1,v2).
    let triangle = triangle + [new_v].
    let ix = ix + 1.
end

for r in triangle do
    println (r @map(lambda with v do return tostring v) @join(" ")).
end
