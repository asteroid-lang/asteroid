-- testing the builtin 'len' function

load system io.
load system type.

let tostring = type @tostring.

structure Foo with
   data one.
   data two.
   data three.
   function hello
      with none do
        io @println (tostring(this @one)+
                 tostring(this @two)+
                 tostring(this @three)).
      end
   end

let ll = len [1,2,3].
assert (ll == 3).
io @println ll.

let lt = len (1,2,3).
assert (lt == 3).
io @println lt.

let ls = len "123".
assert (ls == 3).
io @println ls.

let f = Foo(1,2,3).
f @hello().
let lf = len f.
assert (lf == 3).
let lFoo = len Foo.
assert (lFoo == 3).
