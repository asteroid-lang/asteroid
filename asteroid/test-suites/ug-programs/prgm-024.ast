    load system io.

    let x = tointeger (io @input "Please enter an integer: ").

    if x < 0 do
        let x = 0.
        io @println "Negative, changed to zero".
    elif x == 0 do
        io @println "Zero".
    elif x == 1 do
        io @println "One".
    else do
        io @println "Something else".
    end
